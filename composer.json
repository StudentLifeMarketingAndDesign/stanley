{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "3.6.*@stable",
		"silverstripe/framework": "3.6.*@stable",
		"silverstripe-themes/simple": "*",
		"silverstripe/event-calendar": "*",
		"silverstripe/blog" : "2.*@stable",
		"md/ac-json-events" : "dev-master",
		"silverstripe/mathspamprotection" : "*",
		"jonom/focuspoint" : "*",
		"silverstripe/userforms": "4.*@stable",
		"silverstripe/googlesitemaps" : "*",
		"colymba/gridfield-bulk-editing-tools" : "*",
		"undefinedoffset/sortablegridfield": "*",
		"silverstripe/widgets": "*",
		"silverstripe/lumberjack": "^1.2",
		"silverstripe/activedirectory" : "3.*",
		"md/division-analytics" : "1.*",
		"undefinedoffset/silverstripe-nocaptcha" : "*",
		"unclecheese/betterbuttons":"1.3.*"

	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/ac-json-events"

        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"

        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-activedirectory"

        },
        {
             "type": "vcs",
             "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-betternavigator"
        }
    ]
}
